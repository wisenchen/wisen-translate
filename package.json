{
  "name": "Wisen-Translate",
  "displayName": "Wisen-Translate",
  "description": "鼠标悬浮翻译，内嵌翻译页面",
  "version": "0.0.1",
  "publisher": "wisen",
  "icon": "resource/icon.png",
  "repository": {
    "url": "https://github.com/ezshine/vscode-ilovejuejin"
},
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "wisen translate",
      "type": "object",
      "properties": {
        "wisenTranslate.origin": {
          "type": "string",
          "default": "youdao",
          "description": "设置翻译使用的源，可选填，baidu、youdao、google",
          "enum": [
            "baidu",
            "youdao",
            "google"
          ]
        },
        "wisenTranslate.disableHover": {
          "type": "boolean",
          "default": false,
          "description": "是否禁用鼠标悬浮翻译"
        }
      }
    },
    "commands": [
      {
        "command": "wisen-translate.zh-en",
        "title": "输入中文翻译成英文",
        "category": "Wisen Translate"
      },
      {
        "command": "wisen-translate.translate-highlight",
        "title": "翻译选中文字",
        "category": "Wisen Translate"
      }
    ],
    "keybindings": [
      {
        "command": "wisen-translate.zh-en",
        "key": "alt+t",
        "mac": "alt+t"
      },
      {
        "command": "wisen-translate.translate-highlight",
        "key": "alt+w",
        "mac": "alt+w"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "wisenTranslate",
          "title": "选择翻译",
          "icon": "resource/translate.svg"
        }
      ]
    },
    "views": {
      "wisenTranslate": [
        {
          "id": "wisenTranslate-item",
          "name": "item",
          "when": ""
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.45.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "rxjs": "^6.6.3",
    "translation-api": "^1.0.5"
  }
}
